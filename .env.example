# Voyage GEO — Environment Variables
# Copy this file to .env and fill in your API keys

# OpenRouter (one key for all models — recommended)
OPENROUTER_API_KEY=sk-or-v1-...

# Direct provider keys (optional, for backward compatibility)
# OpenAI (ChatGPT)
OPENAI_API_KEY=sk-...

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-...

# Google (Gemini)
GOOGLE_API_KEY=AI...

# Perplexity
PERPLEXITY_API_KEY=pplx-...

# Optional: Processing model for non-execution LLM calls (research, query gen, analysis)
# Defaults to anthropic / claude-opus-4-6 — uses the API key for the chosen provider
# PROCESSING_PROVIDER=anthropic
# PROCESSING_MODEL=claude-opus-4-6

# Optional: Log level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Output directory for run data
VOYAGE_GEO_OUTPUT_DIR=./data/runs

# Optional: Default concurrency limit
VOYAGE_GEO_CONCURRENCY=3

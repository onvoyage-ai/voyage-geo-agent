  },
  "devDependencies": {
    "@types/node": "^22.13.2",
    "@types/sentiment": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^8.57.1",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",

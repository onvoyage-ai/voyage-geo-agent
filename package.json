{
  "name": "voyage-geo",
  "version": "0.1.0",
  "description": "Open source GEO (Generative Engine Optimization) CLI tool â€” analyze how AI models reference and recommend your brand",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "voyage-geo": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "start": "node dist/cli.js",
    "prepare": "tsup"
  },
  "keywords": [
    "geo",
    "generative-engine-optimization",
    "ai-ranking",
    "brand-analysis",
    "chatgpt",
    "claude",
    "gemini",
    "perplexity",
    "seo",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/genai": "^0.7.0",
    "chalk": "^5.4.1",
    "chart.js": "^4.4.8",
    "chartjs-node-canvas": "^4.1.6",
    "cheerio": "^1.0.0",
    "commander": "^13.1.0",
    "csv-stringify": "^6.5.2",
